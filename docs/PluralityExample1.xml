  <!-- Example: Plurality for numeric argument {0} only -->
  <Key:CatsDogs>
    {0} cat(s) and {1} dog(s)
    <N:Zero>no cats and {1} dog(s)</N:Zero>
    <N:One>a cat and {1} dog(s)</N:One>
    <N:Plural>{0} cats and {1} dog(s)</N:Plural>
  </Key:CatsDogs>

</Localization>
